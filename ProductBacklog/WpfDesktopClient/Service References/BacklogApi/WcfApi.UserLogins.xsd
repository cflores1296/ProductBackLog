      <xs:element minOccurs="0" maxOccurs="unbounded" name="RemovedUserLogin" nillable="true" type="tns:RemovedUserLogin" />
    </xs:sequence>
  </xs:complexType>
  <xs:element name="ArrayOfRemovedUserLogin" nillable="true" type="tns:ArrayOfRemovedUserLogin" />
